{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.9",
        "laravel/cashier": "^14.12",
        "laravel/fortify": "^1.17",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.18",
        "laravel/pennant": "^1.4",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.8",
        "laravel/telescope": "^4.15",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "openai-php/laravel": "^0.7",
        "pusher/pusher-php-server": "^7.2",
        "sokil/php-isocodes": "^4.2",
        "sokil/php-isocodes-db-i18n": "^4.0",
        "soundasleep/html2text": "^2.1",
        "spatie/laravel-data": "^3.7",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-medialibrary": "^10.11",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/pdf-to-text": "^1.52",
        "sqids/sqids": "^0.4.1",
        "staudenmeir/belongs-to-through": "^2.13",
        "yooper/php-text-analysis": "^1.9"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "laravel-lang/common": "^4.0",
        "laravel/envoy": "^2.8",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.6",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
